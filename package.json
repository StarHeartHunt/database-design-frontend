{
  "name": "database-design-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "sourcemap": "source-map-explorer ./dist/*.js",
    "eslint": "eslint --cache --report-unused-disable-directives",
    "eslint:fix": "pnpm eslint --fix",
    "lint": "pnpm lint:js",
    "lint:fix": "pnpm lint:js:fix",
    "lint:js": "pnpm eslint \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "lint:js:fix": "pnpm eslint:fix \"**/*.{js,jsx,ts,tsx,cjs,mjs,vue}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "vue": "^3.3.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.1",
    "eslint-plugin-unicorn": "^50.0.1",
    "eslint-plugin-vue": "^9.19.2",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "rollup-plugin-visualizer": "^5.11.0",
    "source-map-explorer": "^2.5.3",
    "typescript": "^5.3.3",
    "unocss": "^0.58.0",
    "vite": "^5.0.10",
    "vue-tsc": "^1.8.26"
  }
}
